{
  "name": "watermark-web",
  "version": "0.1.3",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@types/jest": "^24.0.23",
    "jest": "^24.9.0",
    "parcel": "^2.8.3",
    "prettier": "^2.8.8",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3"
  },
  "scripts": {
    "test": "jest",
    "tsc": "npm run tsc:lib && npm run tsc:es && npm run tsc:umd",
    "tsc:lib": "rm -rf lib && tsc",
    "tsc:es": "rm -rf es && tsc --outDir es --module es2015",
    "tsc:umd": "rm -rf umd && tsc --outDir umd --module umd",
    "example": "parcel ./example/index.html",
    "build": "parcel build ./example/index.html --public-url . --no-source-maps"
  },
  "keywords": [
    "browser",
    "watermark",
    "typescript"
  ],
  "jest": {
    "preset": "ts-jest"
  }
}
